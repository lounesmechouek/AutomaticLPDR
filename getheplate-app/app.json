{
  "expo": {
    "name": "getheplate-app",
    "slug": "gettheplate-app",
    "owner": "badido",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": false
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera"
        }
      ],
      "react-native-compressor"
    ],
    "extra": {
      "eas": {
        "projectId": "37463fad-5584-470c-8b8c-2707765e8bda"
      }
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.badido.gettheplateapp"
    }
  }
}
