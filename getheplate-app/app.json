{
  "expo": {
    "name": "getheplate-app",
    "slug": "gettheplate-app",
    "owner": "badido",
    "version": "1.0.4",
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": false
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera"
        }
      ],
      "react-native-compressor"
    ],
    "extra": {
      "eas": {
        "projectId": "37463fad-5584-470c-8b8c-2707765e8bda"
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.rn_wp.android",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
    },
    "updates": {
      "url": "https://u.expo.dev/37463fad-5584-470c-8b8c-2707765e8bda"
    }
  }
}
